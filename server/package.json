{
  "name": "boilerplate",
  "version": "1.0.0",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "client": "cd ../client && yarn client",
    "server": "nodemon --exec babel-node src/server.js",
    "dev": "set NODE_ENV=development&& concurrently --kill-others-on-fail \"yarn client\" \"yarn server\""
  },
  "babel": {
    "presets": [
      "env",
      "stage-2"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "concurrently": "^3.5.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "nodemon": "^1.14.6",
    "prettier": "^1.13.5"
  },
  "dependencies": {
    "express": "^4.16.2"
  }
}
